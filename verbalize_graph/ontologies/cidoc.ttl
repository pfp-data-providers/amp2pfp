        rdfs:label          "wurde benutzt für"@de;
        rdfs:label          "χρησιμοποιήθηκε για"@el;
        rdfs:label          "a été mobilisé pour"@fr;
        rdfs:label          "foi usado por"@pt;
        rdfs:label          "был использован"@ru;
        rdfs:label          "用于"@zh;
        rdfs:domain         <E70_Thing>;
