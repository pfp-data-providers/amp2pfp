        owl:inverseOf       <P14_carried_out_by> .

<P15_was_influenced_by>
        rdf:type       rdf:Property;
        rdfs:label     "was influenced by"@en;
        rdfs:label     "wurde beeinflußt durch"@de;
        rdfs:label     "επηρεάστηκε από"@el;
        rdfs:label     "a été influencé par"@fr;
